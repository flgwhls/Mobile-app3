    <TextView
        android:id="@+id/tv_booksellp_publisher"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        android:text="@string/publisher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_isbn" />

    <TextView
        android:id="@+id/tv_booksellp_publisher2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_booksellp_publisher"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_isbn2" />

    <TextView
        android:id="@+id/tv_booksellp_publicyear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="24dp"
        android:text="@string/publication_year"
        app:layout_constraintStart_toEndOf="@+id/iv_booksellp_image"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_publisher2" />

    <TextView
        android:id="@+id/tv_booksellp_publicyear2"
        android:layout_width="40dp"
        android:layout_height="19dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/iv_booksellp_image"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_publicyear" />

    <TextView
        android:id="@+id/tv_booksell_edition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginLeft="48dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="124dp"
        android:layout_marginRight="124dp"
        android:layout_marginBottom="148dp"
        android:text="@string/edition"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/iv_booksellp_image"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_publicyear2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tv_booksellp_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        android:text="@string/category"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_booksellp_title2" />

    <TextView
        android:id="@+id/tv_booksellp_category2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
